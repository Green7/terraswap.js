{
  "name": "terraswap.js",
  "version": "1.0.11",
  "description": "Terraswap.js is simple library for working with TeraSwap automated market-maker (AMM) protocol on the Terra blockchain.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "rimraf dist && npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Green7/terraswap.js.git"
  },
  "keywords": [
    "blockchain",
    "terra",
    "terraswap"
  ],
  "author": "Green7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Green7/terraswap.js/issues"
  },
  "homepage": "https://github.com/Green7/terraswap.js#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "dotenv": "^10.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@terra-money/terra.js": "^3.0.3",
    "axios": "^0.24.0"
  },
  "files": [
    "dist/**/*"
  ]
}
